  <main class="px-4 sm:px-6 lg:px-8">
    <div
      class="text-white">
      <div class="px-4">
        {% block content %}
        {% if content %}
        {{ content | safe }}
        {% else %}
        <section class="hero">
          <p class="eyebrow">Campfire ready</p>
          <h1>Welcome to Campsite</h1>
          <p>Edit <code>src/pages/index.md</code> to get started.</p>
        </section>
        {% endif %}
        {% endblock %}
      </div>
    </div>
  </main>
  <footer class="py-8 text-xs text-amber-50/40 text-center">
    <p>Built with <span class="font-semibold text-amber-50/60">Campsite</span>.</p>
  </footer>
