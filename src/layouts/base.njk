<!doctype html>
<html lang="en" class="h-full min-h-screen">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {% set meta_title = frontmatter.title or title or site.name %}
  {% set meta_description = frontmatter.description or page.description or "A cozy Campsite starter." %}
  {% set meta_url = frontmatter.canonical or frontmatter.url %}
  <title>{{ meta_title }}</title>
  <meta name="description" content="{{ meta_description }}">
  {% if frontmatter.robots %}
  <meta name="robots" content="{{ frontmatter.robots }}">{% endif %}
  {% if meta_url %}
  <link rel="canonical" href="{{ meta_url }}">{% endif %}
  <meta property="og:title" content="{{ meta_title }}">
  <meta property="og:description" content="{{ meta_description }}">
  {% if meta_url %}
  <meta property="og:url" content="{{ meta_url }}">{% endif %}
  <meta property="og:type" content="{{ frontmatter.og_type or " website" }}">
  <meta property="og:site_name" content="{{ site.name }}">
  <meta name="twitter:card" content="{{ frontmatter.twitter_card or " summary_large_image" }}">
  <meta name="twitter:title" content="{{ meta_title }}">
  <meta name="twitter:description" content="{{ meta_description }}">
  {% if meta_url %}
  <meta name="twitter:url" content="{{ meta_url }}">{% endif %}
  <link rel="stylesheet" href="/style.css">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Caprasimo&display=swap" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/@tailwindplus/elements@1" type="module"></script>
</head>
<body class="{{ page.body_css or '' }} relative h-full min-h-screen pt-12">
  <header class="font-serif fixed z-50 w-full top-0 left-0 bg-stone-950 py-2 px-6 flex items-stretch justify-between">
    <a href="/" class="inline-flex items-center text-white text-3xl py-1 hover:underline">
      Câ›ºmpsiteJS
    </a>
    <nav class="flex items-center gap-2 text-white">
      {% set nav_items = navbar or data.navbar or [] %}
      {% for item in nav_items %}
        <a href="{{ item.url or '#' }}"
          {% if item.target %}target="{{ item.target }}"{% endif %}
          {% if item.rel %}rel="{{ item.rel }}"{% endif %}
          class="px-2 {% if item.url == page.path %} active {% else %} not-active {% endif %} {{ item.class }}">
          {% if item.label %}{{ item.label }}{% endif %}
          {% if item.svg %}{{ item.svg | safe }}{% endif %}
        </a>
      {% endfor %}
    </nav>
  </header>
  <main>
    {% block content %}
    {% if content %}
    {{ content | safe }}
    {% else %}
    <section>
      <p>Campfire ready</p>
      <h1>Welcome to Campsite</h1>
      <p>Edit <code>src/pages/index.md</code> to get started.</p>
    </section>
    {% endif %}
    {% endblock %}
  </main>
